WAAA,KAAA,CAAA,QAAA,EAFA,CAEA;;AACA,MAAA,SAAA,CAAA,KAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAHA;AAIA;AARA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <el-table :data=\"list\" border fit highlight-current-row style=\"width: 100%\">\r\n    <el-table-column\r\n      v-loading=\"loading\"\r\n      align=\"center\"\r\n      label=\"ID\"\r\n      width=\"65\"\r\n      element-loading-text=\"请给我点时间！\"\r\n    >\r\n      <template slot-scope=\"scope\">\r\n        <span>{{ scope.row.id }}</span>\r\n      </template>\r\n    </el-table-column>\r\n\r\n    <el-table-column width=\"180px\" align=\"center\" label=\"Date\">\r\n      <template slot-scope=\"scope\">\r\n        <span>{{ scope.row.timestamp | parseTime('{y}-{m}-{d} {h}:{i}') }}</span>\r\n      </template>\r\n    </el-table-column>\r\n\r\n    <el-table-column min-width=\"300px\" label=\"Title\">\r\n      <template slot-scope=\"{row}\">\r\n        <span>{{ row.title }}</span>\r\n        <el-tag>{{ row.type }}</el-tag>\r\n      </template>\r\n    </el-table-column>\r\n\r\n    <el-table-column width=\"110px\" align=\"center\" label=\"Author\">\r\n      <template slot-scope=\"scope\">\r\n        <span>{{ scope.row.author }}</span>\r\n      </template>\r\n    </el-table-column>\r\n\r\n    <el-table-column width=\"120px\" label=\"Importance\">\r\n      <template slot-scope=\"scope\">\r\n        <svg-icon v-for=\"n in +scope.row.importance\" :key=\"n\" icon-class=\"star\" />\r\n      </template>\r\n    </el-table-column>\r\n\r\n    <el-table-column align=\"center\" label=\"Readings\" width=\"95\">\r\n      <template slot-scope=\"scope\">\r\n        <span>{{ scope.row.pageviews }}</span>\r\n      </template>\r\n    </el-table-column>\r\n\r\n    <el-table-column class-name=\"status-col\" label=\"Status\" width=\"110\">\r\n      <template slot-scope=\"{row}\">\r\n        <el-tag :type=\"row.status | statusFilter\">\r\n          {{ row.status }}\r\n        </el-tag>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n</template>\r\n\r\n<script>\r\nimport { fetchList } from '@/api/article'\r\n\r\nexport default {\r\n  filters: {\r\n    statusFilter(status) {\r\n      const statusMap = {\r\n        published: 'success',\r\n        draft: 'info',\r\n        deleted: 'danger'\r\n      }\r\n      return statusMap[status]\r\n    }\r\n  },\r\n  props: {\r\n    type: {\r\n      type: String,\r\n      default: 'CN'\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      list: null,\r\n      listQuery: {\r\n        page: 1,\r\n        limit: 5,\r\n        type: this.type,\r\n        sort: '+id'\r\n      },\r\n      loading: false\r\n    }\r\n  },\r\n  created() {\r\n    this.getList()\r\n  },\r\n  methods: {\r\n    getList() {\r\n      this.loading = true\r\n      this.$emit('create') // for test\r\n      fetchList(this.listQuery).then(response => {\r\n        this.list = response.data.items\r\n        this.loading = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n"],"sourceRoot":"src/views/tab/components"}]}